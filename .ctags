--languages=okp

--langdef=okp
--langmap=okp:.cpy.okp
--regex-okp=/^\s*def\s*([a-zA-Z0-9_]+)/\1/d,definition/
--regex-okp=/^\s*[a-zA-Z0-9_]+\s+([a-zA-Z0-9_]+)\(/\1/d,definition/
--regex-okp=/^\s*class\s*([a-zA-Z0-9_]+)/\1/d,class/
--regex-okp=/^\s*struct\s*([a-zA-Z0-9_]+)/\1/d,struct/
--regex-okp=/^\s*[:a-zA-Z0-9_<>* ]+\s+([a-zA-Z0-9_]+);?\s*=[^=]/\1/v,variable/
--regex-okp=/^\s*[:a-zA-Z0-9_<>* ]+\s+([a-zA-Z0-9_]+);?\s*$/\1/v,variable/
# --regex-okp=/^\s*([a-zA-Z0-9_]+)\s*:=/\1/v,variable/

--regex-okp=/^\s*enum[a \t]+([a-zA-Z0-9_])/\1/e,enum/
